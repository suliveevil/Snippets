{
	// Place your snippets for json here. Each snippet is defined under a snippet name and has a prefix, body and 
	// description. The prefix is what is used to trigger the snippet and the body will be expanded and inserted. Possible variables are:
	// $1, $2 for tab stops, $0 for the final cursor position, and ${1:label}, ${2:another} for placeholders. Placeholders with the 
	// same ids are connected.
	// Example:
	// "Print to console": {
	// 	"prefix": "log",
	// 	"body": [
	// 		"console.log('$1');",
	// 		"$2"
	// 	],
	// 	"description": "Log output to console"
	// }
	"textastic snippet file": {
		"prefix": "tsf",
		"body": [
			"{\n\t\"description\": \"${1:description}\",\n\t\"uuid\": \"${2:uuid}\",\n\t\"completionSets\":[\n\t\t${3:tsni$0}\n\t],\n\t\"contexts\": [\n\t\t${4:tcon}\n\t]\n}"
		],
		"description": "create textastic snippet file content"
	},
	"textastic snippet completionSet": {
		"prefix": "tsn",
		"body": [
			"{\n\t\"name\": \"${1:enviroment}.${2:situation}\",\n\t\"defaultAppend\":\"${3}\",\n\t\"strings\":[\n\t\t{\n\t\t\t\"string\":\"${4:string2match}\",\n\t\t\t\"${5:replace}\":\"${6:snippet}\"\n\t\t}\n\t]\n}"
		],
		"description": "create textastic snippet Set"
	},
	"textastic snippet Context": {
		"prefix": "tco,
		"body": [
			"{\n\t\"description\": \"${1:description}\",\n\t\"scope\":\"${2:source}.${3:python}\",\n\t\"pattern\":\"(${4:regex})\",\n\t\"completionCaptureIndex\":1,\n\t\"completionSetNames\":[\n\t\t\"${5:enviroment}.${6:situation}\"\n\t]\n}"
		],
		"description": "create textastic snippet Context"
	}
}