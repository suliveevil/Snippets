{
    "description": "Textastic CodeCompletion for Git",
    "uuid": "uuid",
    "completionSets": [
        {
            "name": "git.function",
            "defaultAppend": "",
            "strings": [
                {
                    "string": "add",
                    "replace": "\\sadd\\s${1:file}"
                },
                {
                    "string": "branch",
                    "replace": "\\sbranch$0"
                },
                {
                    "string": "check-ignore",
                    "replace": "\\scheck-ignore$0"
                },
                {
                    "string": "checkout",
                    "replace": "\\scheckout$0"
                },
                {
                    "string": "cherry-pick",
                    "replace": "\\scherry-pick$0"
                },
                {
                    "string": "clone",
                    "replace": "\\sclone$0"
                },
                {
                    "string": "commit",
                    "replace": "\\scommit\\s-m\\s${1:commit message}"
                },
                {
                    "string": "config",
                    "replace": "\\sconfig$0"
                },
                {
                    "string": "diff",
                    "replace": "\\sdiff\\s${1:file}"
                },
                {
                    "string": "init",
                    "replace": "\\sinit$0"
                },
                {
                    "string": "log",
                    "replace": "\\slog$0"
                },
                {
                    "string": "merge",
                    "replace": "\\smerge$0"
                },
                {
                    "string": "pull",
                    "replace": "\\spull$0"
                },
                {
                    "string": "push",
                    "replace": "\\spush$0"
                },
                {
                    "string": "rebase",
                    "replace": "\\srebase$0"
                },
                {
                    "string": "reflog",
                    "replace": "\\sreflog$0"
                },
                {
                    "string": "remote",
                    "replace": "\\sremote$0"
                },
                {
                    "string": "reset",
                    "replace": "\\sreset$0"
                },
                {
                    "string": "rm",
                    "replace": "\\srm$0"
                },
                {
                    "string": "show",
                    "replace": "\\sshow$0"
                },
                {
                    "string": "stash",
                    "replace": "\\sstash$0"
                },
                {
                    "string": "status",
                    "replace": "\\sstatus$0"
                },
                {
                    "string": "switch",
                    "replace": "\\sswitch$0"
                },
                {
                    "string": "tag",
                    "replace": "\\stag$0"
                },
                {
                    "//" :"alias-自定义命令名称"
                    "string": "unstage",
                    "replace": "\\sunstage\\${1:file}"
                }
            ]
        },
        {
            "name": "git.config.parameters",
            "defaultAppend": "",
            "strings": [
                {
                    "string": "global",
                    "replace": "--global"
                }
            ]
        },
        {
            "name": "git.log.parameters",
            "defaultAppend": "",
            "strings": [
                {
                    "string": "pretty",
                    "replace": "\\s--pretty=oneline"
                }
            ]
        },
        {
            "name": "git.pull.parameters",
            "defaultAppend": "",
            "strings": [
                {
                    "string": "origin",
                    "replace": "\\sorigin\\s${1:branch}"
                }
            ]
        },
        {
            "name": "git.remote.parameters",
            "defaultAppend": "",
            "strings": [
                {
                    "string": "add",
                    "replace": "\\sadd"
                },
                {
                    "string":"origin",
                    "replace":"\\sorigin"
                }
            ]
        },
        {
            "name": "git.stash.parameters",
            "defaultAppend": "",
            "strings": [
                {
                    "string": "apply",
                    "replace": "\\sapply"
                },
                {
                    "string": "list",
                    "replace": "\\slist$0"
                },
                {
                    "string": "pop",
                    "replace": "\\spop$0"
                }
                
            ]
        }
    ],
    "contexts": [{
        "description": "description",
        "scope": "source.shell text.html.markdown",
        "pattern": "(prefix[a-zA-Z0-9]*)",
        "completionCaptureIndex": 1,
        "completionSetNames": [
            "enviroment.situation1.situation2"
        ]
    }]
}